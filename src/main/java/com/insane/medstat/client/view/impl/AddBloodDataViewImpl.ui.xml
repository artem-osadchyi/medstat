<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bdtp="urn:import:org.gwtbootstrap3.extras.datetimepicker.client.ui"
>
	<ui:style>
	
	</ui:style>
	<b:Container>
		<b:Row>
			<b:Panel ui:field="panel">
				<b:PanelBody>
					<b:Form>
						<b:FormGroup>
							<b:FormLabel
								text="ID"
								for="idInput" />
							<b:Input
								ui:field="idInput"
								type="NUMBER" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Patient ID"
								for="patientIdInput" />
							<b:Input
								ui:field="patientIdInput"
								type="NUMBER" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Time"
								for="timestampPicker" />
							<bdtp:DateTimePicker
								ui:field="timestampPicker"
								autoClose="true"
								highlightToday="true"
								weekStart="MONDAY"
								showTodayButton="true"
								minuteStep="30"
								maxView="MONTH"
								language="RU"
								hasKeyboardNavigation="true"
								forceParse="true" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Systolic Blood Pressure"
								for="systolicInput" />
							<b:Input
								ui:field="systolicInput"
								type="NUMBER" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Diastolic Blood Pressure"
								for="diastolicInput" />
							<b:Input
								ui:field="diastolicInput"
								type="NUMBER" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Pulse"
								for="pulseInput" />
							<b:Input
								ui:field="pulseInput"
								type="NUMBER" />
						</b:FormGroup>
						<b:FormGroup>
							<b:FormLabel
								text="Comment"
								for="commentInput" />
							<b:Input
								ui:field="commentInput"
								type="TEXT" />
						</b:FormGroup>
						<b:FormGroup>
							<b:Button
								ui:field="submitButton"
								text="Submit"
								type="PRIMARY" />
						</b:FormGroup>
					</b:Form>
				</b:PanelBody>
			</b:Panel>
		</b:Row>
	</b:Container>
</ui:UiBinder>